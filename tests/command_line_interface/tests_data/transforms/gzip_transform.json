{
    "type": "csv",
    "settings": {
        "is_default": false,
        "compression": "gzip",
        "format_details": {
            "delimiter": ",",
            "skip_head": 1,
            "buffer_size": 4096,
            "windows_ending": true
        },
        "output_columns": [
            {
                "name": "_timestamp",
                "datatype": {
                    "primary": true,
                    "type": "datetime",
                    "format": "02/01/2006 15:04",
                    "resolution": "seconds",
                    "source": {
                        "from_input_index": 0
                    }
                }
            },
            {
                "name": "_float",
                "datatype": {
                    "type": "double",
                    "source": {
                        "from_input_index": 1
                    }
                }
            },
            {
                "name": "_ip4",
                "datatype": {
                    "type": "string",
                    "source": {
                        "from_input_index": 2
                    }
                }
            },
            {
                "name": "_key",
                "datatype": {
                    "type": "string",
                    "source": {
                        "from_input_index": 3
                    }
                }
            },
            {
                "name": "_numeric",
                "datatype": {
                    "type": "uint64",
                    "source": {
                        "from_input_index": 4
                    }
                }
            },
            {
                "name": "_string",
                "datatype": {
                    "type": "string",
                    "source": {
                        "from_input_index": 5
                    }
                }
            },
            {
                "name": "_uint64",
                "datatype": {
                    "type": "uint64",
                    "source": {
                        "from_input_index": 6
                    }
                }
            }
        ]
    }
}